/dts-v1/;
/plugin/;

/ {
   	compatible = "st,sta1385";

	fragment@1 {
                target = <&ethernet0>;
		__overlay__ {
			phy-mode = "rmii";
			snps,en-gpio = <&gpio1 4 0>;
		};
        };

};
